{
  "editor.tabSize": 2,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "eslint.nodePath": "../node_modules",
  "eslint.validate": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact"
  ],
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "css.validate": false
}
